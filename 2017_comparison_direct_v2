/**** Start of imports. If edited, may not auto-convert in the playground. ****/
var nlcd = ee.ImageCollection("USGS/NLCD"),
    image = ee.Image("projects/ee-valeriepasquarella/assets/ForestCondition/2017_reanalysis_meanresiduals"),
    image2 = ee.Image("projects/ee-valeriepasquarella/assets/ForestCondition/asssessment_features-v4-2_tcg_2000-2010_NEWENGLAND_h13_full_2017-05-01_2017-09-30"),
    image3 = ee.Image("projects/sites-project/asssessment_features-v4-2_sr_2000-2010_NEWENGLAND_h13_16d_2017-05-01_2017-09-30"),
    table = ee.FeatureCollection("projects/ee-valeriepasquarella/assets/wrs2_descending"),
    table2 = ee.FeatureCollection("projects/ee-valeriepasquarella/assets/CT-GM/FEN_BurlapSurvey_Coords"),
    table3 = ee.FeatureCollection("projects/ee-valeriepasquarella/assets/HF-GM/PlotDefHarvSimple");
/***** End of imports. If edited, may not auto-convert in the playground. *****/
var tcc = nlcd.filterDate('2016').first().select('percent_tree_cover')
var mask = tcc.gte(75)

var VIZ_MIN = -4;
var VIZ_MAX = 0;
var PALETTE = ['red', 'orange', 'yellow', 'blue']
var viz = {min: VIZ_MIN, max: VIZ_MAX, palette: PALETTE};

Map.addLayer(image.updateMask(mask), viz, 'reanalysis product')
Map.addLayer(image2.updateMask(mask), viz, 'tcg_2000-2010_NEWENGLAND_h13_full')
Map.addLayer(image3.updateMask(mask), viz, 'sr_2000-2010_NEWENGLAND_h13_16d')
Map.addLayer(table,{}, 'WRS2')
Map.addLayer(table2, {}, 'CT plots')
Map.addLayer(table3, {}, 'MA plots')

var style = {
  'Figure': [{
      featureType: 'all',
      stylers: [{ color: '#000000'}]
  }]
};

// Set map options.
Map.setOptions(null, style);