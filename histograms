/**** Start of imports. If edited, may not auto-convert in the playground. ****/
var imageCollection = ee.ImageCollection("projects/sites-project/condition_monitoring/products_v9-0_by_state_annual"),
    STATES = ee.FeatureCollection("TIGER/2018/States");
/***** End of imports. If edited, may not auto-convert in the playground. *****/
var STATE = 'MA';
var YEAR = 2019;

var map = imageCollection.filterMetadata('state', 'equals', STATE)
    .filterMetadata('monitor_year', 'equals', YEAR)
    .first()
    .select('score_mean');

var state_roi = STATES
    .filterMetadata('STUSPS', 'equals', STATE)
    .first().geometry(); 

var histogram = ui.Chart.image.histogram(map, state_roi, 30, null, null, null, 1e10);
print(histogram);