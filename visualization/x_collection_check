/**** Start of imports. If edited, may not auto-convert in the playground. ****/
var imageCollection = ee.ImageCollection("projects/sites-project/baselines_v6-3_by_state");
/***** End of imports. If edited, may not auto-convert in the playground. *****/

print('MA', imageCollection
  .filterMetadata('state', 'equals', 'MA')
  .aggregate_array('name').distinct())
  
print('PA', imageCollection
  .filterMetadata('state', 'equals', 'PA')
  .aggregate_array('name').distinct())
  
print('CT', imageCollection
  .filterMetadata('state', 'equals', 'CT')
  .aggregate_array('name').distinct())
  
print('RI', imageCollection
  .filterMetadata('state', 'equals', 'RI')
  .aggregate_array('name').distinct())
  

Map.addLayer(imageCollection
  .filterMetadata('state', 'equals', 'MA'))
Map.addLayer(imageCollection
  .filterMetadata('state', 'equals', 'PA'))
Map.addLayer(imageCollection
  .filterMetadata('state', 'equals', 'CT'))
Map.addLayer(imageCollection
  .filterMetadata('state', 'equals', 'RI'))